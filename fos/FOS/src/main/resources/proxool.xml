<?xml version="1.0" encoding="UTF-8"?>
<proxool-config>
	<proxool>
		<alias>fosdb</alias>
		<driver-url>
			jdbc:mysql://localhost/FOS4?useUnicode=true&amp;characterEncoding=utf-8
			  <!-- jdbc:mysql://192.168.1.100/fos4?useUnicode=true&amp;characterEncoding=utf-8  -->
		</driver-url>
		<driver-class>com.mysql.jdbc.Driver</driver-class>
		<driver-properties>
			<property name="user" value="fos" />
			<property name="password" value="fos" />
		</driver-properties>
		<maximum-connection-count>50</maximum-connection-count>
		<minimum-connection-count>5</minimum-connection-count>
		<house-keeping-sleep-time>90000</house-keeping-sleep-time>
		<prototype-count>5</prototype-count>
	</proxool>
</proxool-config>