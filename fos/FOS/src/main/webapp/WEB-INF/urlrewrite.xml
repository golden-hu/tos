<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	
	<rule>
		<from>/channel/*</from>
		<to>/app/front?_A=CHAN_Q&amp;CODE=$1</to>
	</rule>
	<rule>
		<from>/service/*</from>
		<to>/app/front?_A=$1</to>
	</rule>
	<rule>
		<from>/</from>
		<to>/app/front</to>
	</rule>
	<rule>
		<from>/front</from>
		<to>/app/front</to>
	</rule>
</urlrewrite>
