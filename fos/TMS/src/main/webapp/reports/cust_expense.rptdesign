            <xml-property name="queryText"><![CDATA[select e.ID, e.CUST_NAME, 
e.CONS_NO, e.CHAR_NAME, 
e.EXPE_TOTAL_AMOUNT,
(e.EXPE_TOTAL_AMOUNT-EXPE_WRITE_OFF_AMOUNT) as NOT_WRITE_OFF_AMOUNT,
e.CONS_DATE,e.EXPE_INVOICE_DATE,
e.EXPE_INVOICE_NO, e.EXPE_TAX_INVOICE_NO, e.CONS_SALES_REP_NAME,
cu.CUST_CREDIT_DAY, e.GROU_NAME
from S_EXPENSE e, C_CUSTOMER cu
where e.CUST_ID = cu.ID
and e.EXPE_TYPE = "R"
AND e.removed = "0"
]]></xml-property>
