            <method name="beforeOpen"><![CDATA[var sql = this.queryText;
if(params["dt"] == "0"){
	sql += " and e.CONS_DATE >= '" + params["F"] + "'";
	sql += " and e.CONS_DATE <= '" + params["T"] + "'";
}else{
	sql += " and e.EXPE_INVOICE_DATE >= '" + params["F"] + "'";
	sql += " and e.EXPE_INVOICE_DATE <= '" + params["T"] + "'";
}
if(params["custId"] != ""){
	sql += " and e.CUST_ID = " + params["custId"];
}

if(params["charName"] != ""){
	sql += " and e.CHAR_NAME = '" + params["charName"] + "'";
}
if(params["consSalesRepId"] != ""){
	sql += " and e.CONS_SALES_REP_ID = " + params["consSalesRepId"];
}
if(params["expeBillStatus"] != ""){
	sql += " and e.EXPE_BILL_STATUS = '" + params["expeBillStatus"] + "'";
}
if(params["expeInvoiceStatus"] != ""){
	sql += " and e.EXPE_INVOICE_STATUS = '" + params["expeInvoiceStatus"] + "'";
}
if(params["expeWriteoffStatus"] != ""){
	sql += " and e.EXPE_WRITEOFF_STATUS = '" + params["expeWriteoffStatus"] + "'";
}
sql += " and e.COMP_CODE = '" + params["compCode"] + "'";
this.queryText = sql;]]></method>
