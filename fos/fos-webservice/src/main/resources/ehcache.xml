		maxElementsInMemory="1000"
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600"
		overflowToDisk="true" />
	<cache name="haitai.fosws.entity.table.WInquiry" 
		maxElementsInMemory="1000"
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600"
		overflowToDisk="true" />
	<cache name="haitai.fosws.entity.table.WUser" 
		maxElementsInMemory="1000"
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600"
		overflowToDisk="true" />

</ehcache>